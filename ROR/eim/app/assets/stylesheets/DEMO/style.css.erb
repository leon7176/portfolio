.people-profile{position:relative;}
.people-profile img.profile{float:left;margin-right:20px;}
.people-profile h2{display:inline-block;color:#303030;font-size:20px;line-height:50px;}
.people-profile .detail{float:left;margin-right:60px;padding-left:5px;border-left:1px solid #303030;font-size:13px;line-height:18px;}

.event-header h3{font-size:30px;color:#303030;font-family:'OpenSans Light';}
.event-header h2{font-size:36px;color:#303030;font-family:'OpenSans Light';}


ul.switch-date li{height:34px;line-height:34px;font-size:15px;color:#303030;display:inline-block;vertical-align:top;background-color:#e8e8e8;}
ul.switch-date li p{padding:0 20px;}
ul.switch-date li a{display:block;width:34px;height:34px;background-position:center center;background-repeat:no-repeat;}
/*a.prev-week{background-image:url("<%= asset_path 'DEMO/img/prev-event.png' %>");}
a.next-week{background-image:url("<%= asset_path 'DEMO/img/next-event.png' %>");}*/
a.prev{background-image:url("<%= asset_path 'DEMO/img/prev-event.png' %>");}
a.next{background-image:url("<%= asset_path 'DEMO/img/next-event.png' %>");}

ul.month-week{display:block;float:left;font-size:0;float:right;}
ul.month-week li{position:relative;display:inline-block;vertical-align:top;cursor:pointer;padding:0 20px;width:40px;height:32px;line-height:32px;font-size:15px;text-align:center;color:#303030;background-color:#e8e8e8;margin-right:1px;font-family:'OpenSans Light';}
ul.month-week li:hover{background-color:#fbfbfb;}
ul.month-week li.active{color:white;background-color:#59b4cd;}


div.sales{background-color:#acd9e6;}
div.design{background-color:#b7e8bf;}
div.marketing{background-color:#ffb2b2;}

ul.window li{display:none;}
ul.window li.active{display:block;}

