input[type=checkbox]{display:none;}

.line{border-top:1px solid #c9c9c9;border-bottom:1px solid #fbfbfb;}

.form-next{float:left;width:235px;padding-left:40px;border-left:1px solid #bfbfbf;margin-top:60px;}

.links a{display:inline-block;margin:0 20px;font-size:14px;color:#376fca;text-decoration:underline;}

form p{font-size:15px;line-height:30px;font-family:'OpenSans Light';}

.hight{color:#70d27f;}

